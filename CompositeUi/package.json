    "stylelint": "^8.0.0",
    "stylelint-order": "^0.6.0",
    "stylelint-scss": "^2.0.1",
    "stylelint-selector-bem-pattern": "^1.1.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "svg-sprite-loader": "^3.1.2",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.5.9",
