  "scripts": {
    "start": "babel-node scripts/start.js",
    "build": "babel-node scripts/build.js",
    "test": "babel-node scripts/test.js --env=jsdom"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/__mocks__/localStorage.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|config|etc|node_modules|public|scripts)[/\\\\]"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(scss|css)$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.scss$": "<rootDir>/src/__mocks__/styleMock.js"
    }
  },
  "babel": {
    "presets": [
      "stage-3",
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "./config/babelRelayPlugin"
    ]
  },
  "dependencies": {
    "classnames": "^2.2",
    "draft-js": "^0.10",
    "draftjs-to-html": "^0.6.3",
    "history": "^4.6.0",
    "lin3s-css-grid": "^0.3.0",
    "lodash": "^4.17",
    "lodash.debounce": "^4.0.8",
    "mousetrap": "^1.6.0",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draft-wysiwyg": "^1.7.6",
    "react-redux": "^5.0.3",
    "react-relay": "^0.9.3",
    "react-router": "^2.0.0",
    "react-router-redux": "^2.1.0",
    "redux": "^3.6.0",
    "redux-form": "^6.2.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-relay-plugin": "^0.11.0",
    "babel-runtime": "^6.25.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-spawn": "^5.1.0",
    "css-loader": "^0.28.4",
    "detect-port": "^1.2.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.4.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "filesize": "^3.5.10",
    "fs-extra": "^4.0.1",
    "gzip-size": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "imports-loader": "^0.7.1",
    "jest": "^20.0.4",
